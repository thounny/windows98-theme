{
    "name": "Windows98Theme",
    "type": "light",
    "colors": {
        "editor.background": "#C3C7CB",
        "editor.foreground": "#000000",
        "editor.lineHighlightBackground": "#D3D3D3",
        "activityBar.background": "#B0B0B0",
        "sideBar.background": "#B0B0B0",
        "sideBar.foreground": "#000080",
        "button.background": "#00A2E8",
        "statusBar.background": "#808080",
        "tab.activeBackground": "#C3C7CB",
        "tab.inactiveBackground": "#D3D3D3",
        "tab.border": "#808080"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#7F7F7F"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#000080"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#008000"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#A020F0",
				"fontStyle": "bold"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#FFFF00"
            }
        }
    ]
}
